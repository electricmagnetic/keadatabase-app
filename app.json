{
  "expo": {
    "name": "Kea App",
    "slug": "keadatabase-app",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.electricmagnetic.keaapp",
      "buildNumber": "0.0.9",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Enables the Kea App to use your location for kea observations and surveys"
      }
    },
    "android": {
      "package": "com.electricmagnetic.keaapp",
      "versionCode": 13,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC-s5X6sRd5fBrXJIQ0gmTP1Rq_One7gug"
        }
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
